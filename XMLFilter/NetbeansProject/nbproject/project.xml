<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>JasonXMLFilter</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Não usar personalizador das Propriedades do Projeto ao editar este arquivo manualmente. 
 Para impedir que o personalizador seja exibido, crie o arquivo nbproject/project.properties e digite a propriedade
auxiliary.show.customizer=false 
lá. Adicionar 
auxiliary.show.customizer.message=<message>
 mostrará sua mensagem personalizada quando alguém tentar abrir o personalizador.  -->
            <name>JasonXMLFilter</name>
            <properties>
                <property name="ant.script">Jason-1.4.1/build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>JasonXMLFilter</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Jason-1.4.1/src</label>
                    <type>java</type>
                    <location>Jason-1.4.1/src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>compile</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>${ant.script}</script>
                    <target>apidoc</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
                    <target>run</target>
                </action>
                <action name="test">
                    <script>${ant.script}</script>
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>compile</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>Jason-1.4.1/src</label>
                        <location>Jason-1.4.1/src</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>Jason-1.4.1/src</package-root>
                <classpath mode="compile">Jason-1.4.1/src</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
